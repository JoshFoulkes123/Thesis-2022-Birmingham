	<!-- * * * Joint Definitions * * * -->

	<joint name="ground_base" type="fixed">
    	<parent link="ground"/>
    	<child link="base_link"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>

	</joint>

	<joint name="base_hip1" type="fixed">
    	<parent link="base_link"/>
    	<child link="hip_fixed"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>

	</joint>

	<joint name="hip1_hip2" type="revolute">
    	<parent link="hip_fixed"/>
    	<child link="hip"/>
    	<origin xyz="0.016 0 0.265" rpy="0 0 0"/>
		<axis xyz="0 0 1"/>
		<limit lower="-1.3962634" upper="1.3962634" effort="10" velocity="3"/>
	</joint>

	<joint name="hip2_hipjoint" type="revolute">
    	<parent link="hip"/>
    	<child link="hip_joint"/>
    	<origin xyz="0.053 0 0.009" rpy="0 0 0"/>
		<axis xyz="0 1 0"/>
		<limit lower="-1.74532925" upper="0.872664626" effort="10" velocity="3"/>
	</joint>

	<joint name="hip_femur" type="fixed">
    	<parent link="hip_joint"/>
    	<child link="femur"/>
    	<origin xyz="0.067 0.005 0" rpy="0 0 0"/>
	</joint>

	<joint name="femur_uknee" type="fixed">
    	<parent link="femur"/>
